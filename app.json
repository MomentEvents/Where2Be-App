{
  "expo": {
    "name": "Where2Be",
    "slug": "MomentEvents",
    "version": "1.1.1",
    "owner": "moment-app",
    "privacy": "public",
    "description": "Find Events on Campus",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "config": {
        "usesNonExemptEncryption": false
      },
      "supportsTablet": false,
      "bundleIdentifier": "com.momentevents.moment",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.momentevents.moment",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "You can share your photos with your friends."
        }
      ],
      "sentry-expo"
    ],
    "extra": {
      "eas": {
        "projectId": "c26d4688-b61a-4d6b-acd1-2d6cbb13ad2f"
      }
    }
  }
}